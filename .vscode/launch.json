    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Consultant Loader",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/data_questionnaire_agent/bootstrap/consultant_loader.py",
            "console": "integratedTerminal",
            "args": ["bootstrap-photos"]
        },
        {
            "name": "Python Debugger: Start Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/data_questionnaire_agent/server/questionnaire_server_main.py",
            "console": "integratedTerminal"
        }
    ]
}