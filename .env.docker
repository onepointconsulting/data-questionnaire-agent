# DB_HOST=127.0.0.1
# DB_PORT=5432
# DB_PASSWORD=<pwd>
# DB_CREATE=true

# Websocket server related
WEBSOCKET_SERVER=0.0.0.0
WEBSOCKET_PORT=8085
WEBSOCKET_CORS_ALLOWED_ORIGINS=*

# Webserver related
UI_FOLDER=/app/ui

# Langsmith related
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=Data Questionnaire Agent
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
