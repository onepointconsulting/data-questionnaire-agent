# OPENAI_API_KEY=<key>
OPENAI_MODEL=gpt-4o
REQUEST_TIMEOUT=300

VERBOSE_LLM=true
LANGCHAIN_CACHE=false
CHATGPT_STREAMING=false

UI_TIMEOUT = 60

# Email related
# Production Settings
MAIL_FROM_PERSON=Onepoint Data Wellness
# MAIL_USER=<user>
# MAIL_PASSWORD=<pwd>
# MAIL_FROM=<email>
# MAIL_SERVER=<some_server>
MAIL_SUBJECT=Your recommendations from the Onepoint Data Wellness Companionâ„¢
FEEDBACK_EMAIL=<feedback_email>

# General stuff
PROJECT_ROOT=/app
QUESTION_CACHE_FOLDER=/tmp/data-questionnaire-agent/cache

# PDF Related
WKHTMLTOPDF_BINARY=/usr/bin/wkhtmltopdf
TEMPLATE_LOCATION=/app/templates
PDF_FOLDER=/tmp/data_questionnaire_agent/pdfs
PDF_BANNER=/app/public/images/Hero_Image_with_Logo_and_Titles.jpg

# Embedding related
RAW_TEXT_FOLDER=/app/docs/raw_text
EMBEDDINGS_PERSISTENCE_DIR=/app/embeddings
EMBEDDINGS_CHUNK_SIZE=2500
SEARCH_RESULTS_HOW_MANY=2

# Question generation related
QUESTIONS_PER_BATCH=1
MINIMUM_QUESTIONNAIRE_SIZE=4

# Token limit for chatgpt 4. Important to extend the context as much as possible using the vector DB search
TOKEN_LIMIT=6000

# Show session cost
SHOW_SESSION_COST=false
OPENAI_RETRY_ATTEMPTS=3
OPENAI_WAIT_FIXED=30

# Database related
# DB_NAME=data_wellness_companion
